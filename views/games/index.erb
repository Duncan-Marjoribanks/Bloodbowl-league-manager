<h1>Games Played</h1>
<table>
  <tr>
    <th>Team A</th>
    <th>Team B</th>
    <th>Winner</th>
  </tr>
  <% for game in @games %>
  <tr>
    <td><a href='/game/<%game.id%>'>
    <%=game.team(game.team_a,"team_a").name%></a>
  </td>
    <td><a href='/game/<%game.id%>'>
    <%=game.team(game.team_b,"team_b").name%></a>
  </td>
    <td><a href='/game/<%game.id%>'>
    <%=game.team(game.winner, "winner").name%></a>
  </td>
  </tr>
  <%end %>
</table>
